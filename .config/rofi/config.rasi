@import "./catppuccin-default.rasi"

configuration {
  modes: "window,drun,run,ssh";
  font: "MapleMono NF CN 15";
  location: 0;
  yoffset: 0;
  xoffset: 0;
  fixed-num-lines: true;
  show-icons: true;
  timeout {
    action: "kb-cancel";
    delay:  0;
  }
  filebrowser {
    directories-first: true;
    sorting-method:    "name";
  }
}
